{% extends 'baseback.html.twig' %}
{% block body %}
    {{ form_start(formpromos,{'attr':{'novalidate':'novalidate'}}) }}
    <div >
        <a href="{{ path('promoslist') }}" class="mdi mdi-keyboard-backspace"  > </a>
    </div>

    <h4  class="card-title">add promos </h4>

<table class="table">
    <tr>
        <td>
            {{ form_label(formpromos.name,"name") }}
        </td>
        <td>
            {{ form_widget(formpromos.name) }}
            {{ form_errors(formpromos.name) }}
        </td>
    </tr>
    <tr>
        <td>
            {{ form_label(formpromos.datefin,"datefin") }}
        </td>
        <td>
            {{ form_widget(formpromos.datefin) }}
            {{ form_errors(formpromos.datefin) }}
        </td>
    </tr>
</table>


{{ form_end(formpromos) }}


{% endblock %}
